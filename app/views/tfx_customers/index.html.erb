<p id="notice"><%= notice %></p>

<h1>Listing Tfx Customers</h1>

<table>
  <thead>
    <tr>
      <th>Custoemrid</th>
      <th>Logincode</th>
      <th>Customerchainid</th>
      <th>Customername</th>
      <th>Customersiteid</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Phone</th>
      <th>Hosthotelid</th>
      <th>C customerid</th>
      <th>Countrycode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tfx_customers.each do |tfx_customer| %>
      <tr>
        <td><%= tfx_customer.CustoemrID %></td>
        <td><%= tfx_customer.LoginCode %></td>
        <td><%= tfx_customer.CustomerChainID %></td>
        <td><%= tfx_customer.CustomerName %></td>
        <td><%= tfx_customer.CustomerSiteID %></td>
        <td><%= tfx_customer.Address1 %></td>
        <td><%= tfx_customer.Address2 %></td>
        <td><%= tfx_customer.City %></td>
        <td><%= tfx_customer.State %></td>
        <td><%= tfx_customer.Zip %></td>
        <td><%= tfx_customer.Phone %></td>
        <td><%= tfx_customer.HostHotelID %></td>
        <td><%= tfx_customer.C_CustomerID %></td>
        <td><%= tfx_customer.CountryCode %></td>
        <td><%= link_to 'Show', tfx_customer %></td>
        <td><%= link_to 'Edit', edit_tfx_customer_path(tfx_customer) %></td>
        <td><%= link_to 'Destroy', tfx_customer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tfx customer', new_tfx_customer_path %>
